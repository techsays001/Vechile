                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.893" />

            <TextView
                android:id="@+id/phmsg"
                android:layout_width="51dp"
                android:layout_height="47dp"
                android:drawableStart="@drawable/sent"
                android:drawableLeft="@drawable/sent"
